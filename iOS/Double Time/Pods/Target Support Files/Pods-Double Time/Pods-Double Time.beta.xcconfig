FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/BuddyBuildSDK"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/BuddyBuildSDK"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited)  -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/BuddyBuildSDK"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "AssetsLibrary" -framework "BuddyBuildSDK" -framework "CoreTelephony" -framework "CoreText" -framework "SystemConfiguration"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Double Time
PODS_ROOT = ${SRCROOT}/Pods